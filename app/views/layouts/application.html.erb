<!DOCTYPE html>
<html>
<head>
  <title>Urm</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="header">
 <% flash.each do |key, value| -%>
  <div class="flash <%= key %>"><%=h value %></div>
 <% end -%>
 <%# if signed_in? -%>
  <%#= link_to "Sign out", sign_out_path, :method => :delete %>
 <%# else -%>
  <%#= link_to "Sign in", sign_in_path %>
 <%# end -%>
</div>

<%= yield %>

</body>
</html>
