!!!
%html
 %head
  %title Urm
  = stylesheet_link_tag :all
  = javascript_include_tag :defaults
  = csrf_meta_tag
 %body
  #header
   - flash.each do |key, value|
    .flash{:class => key}=h value
   - if logged_in?
    = button_to "Sign out", admin_sessions_path, :method => :delete
   - else
    = link_to "Sign in", new_admin_session_path
  #content
   = yield

