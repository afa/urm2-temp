if($("tr.dms_#{@after}").length > 0){
$("tr.dms_#{@after}").remove();
}
$("#{escape_javascript(@items.empty? ? render(:partial => "dms_empty", :locals => {:after => @after}) : render( :partial => "dms_line", :collection => @items, :locals => {:after => @after}))}").insertAfter($("tr.#{@after}").last());
- unless @items.empty?
 $("#{escape_javascript(render(:partial => "dms_header"))}").insertAfter($("tr.#{@after}").last());
 if($("tr.gap_#{@after}").length == 0  ){
 $("#{escape_javascript(render(:partial => "gap_line"))}").insertAfter($("tr.dms_#{@after}").add("tr.analog_#{@after}").add("tr.info_#{@after}").last());
 }
$("tr.dms_#{@after} th .plus").click(function(){
$("tr.dms_#{@after}").remove();
if($("tr.analog_#{@after}").add("tr.info_#{@after}").length == 0){
$("tr.gap_#{@after}").remove();
}
});
