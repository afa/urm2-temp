- stores.each do |store|
 - p "---crtcls", cart.map{|i| i.class.name }
 - if cart.detect{|i| (not (i.is_a?(CartRequest) || i.is_a?(CartWorld))) && i.location_link == store }
  %tr
   %td{:colspan => 16}= store
  = render :partial => "carts/cart_line.html.haml", :collection => cart.select{|i| (not i.is_a?(CartRequest)) && i.location_link == store }, :locals => {:app_list => app_list}
- if cart.detect{|i| i.is_a?(CartWorld)}
 %tr
  %td{:colspan => 16}= t :cart_world
 = render :partial => "carts/cart_line.html.haml", :collection => cart.select{|i| i.is_a?(CartWorld)}, :locals => {:app_list => app_list}
- if cart.detect{|i| i.is_a?(CartRequest) }
 %tr
  %td{:colspan => 16}= t :requests
 = render :partial => "carts/cart_line.html.haml", :collection => cart.select{|i| i.is_a?(CartRequest) }, :locals => {:app_list => app_list}

